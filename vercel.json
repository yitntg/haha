{
  "version": 2,
  "framework": "vite",
  "routes": [
    {
      "src": "/assets/(.*)\\.js",
      "headers": {
        "content-type": "application/javascript; charset=utf-8"
      },
      "continue": true
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 